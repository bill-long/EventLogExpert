{
  "productName": "eventlogexpert",
  "directories": {
    "output": "app-builds"
  },
  "files": [
    "**/*",
    "!*.psd",
    "!*.ts",
    "!*.code-workspace",
    "!LICENSE.md",
    "!package.json",
    "!package-lock.json",
    "!src/",
    "!e2e/",
    "!hooks/",
    "!.angular-cli.json",
    "!_config.yml",
    "!karma.conf.js",
    "!tsconfig.json",
    "!tslint.json"
  ],
  "win": {
    "extraResources": [
      "./eventutils"
    ],
    "icon": "dist",
    "target": [
      "nsis"
    ],
    "publish": [
      {
        "provider": "github",
        "repo": "EventLogExpert",
        "owner": "bill-long"
      }
    ]
  }
}